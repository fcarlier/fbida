	<Key>plus:		Zoom(inc)		\n\
	<Key>minus:		Zoom(dec)		\n\
	<Key>U:			Undo()			\n\
	~Alt ~Ctrl<Key>C:	Filter(crop)		\n\
	~Alt ~Ctrl<Key>V:	Filter(flip-vert)	\n\
	<Key>H:			Filter(flip-horz)	\n\
	~Alt ~Shift<Key>T:	Filter(rotate-cw)	\n\
