	<Btn3Up>:		Popup(control)		\n\
	\
	<Key>G:			Gamma()			\n\
	<Key>F:			Browser()		\n\
	<Key>L:			Filelist()		\n\
	Ctrl<Key>V:		Ipc(paste)		\n\
