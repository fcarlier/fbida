	Alt<Key>T:		Rotate()		\n\
	<Key>I:			Filter(invert)		\n\
	~Alt<Key>E:		Color()			\n\
	Alt<Key>E:		F3x3(-1,-1,-1,-1,8,-1,-1,-1,-1) \n\
